prof:hasArtifact rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/dx/prof/hasResource
prof:hasResource rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/dx/prof/hasRole
prof:hasRole rdf:type owl:AnnotationProperty .


#################################################################
#    Classes
#################################################################

###  http://www.w3.org/ns/dx/prof/Profile
prof:Profile rdf:type owl:Class .


###  http://www.w3.org/ns/dx/prof/ResourceDescriptor
prof:ResourceDescriptor rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  https://w3id.org/schematransform/ExampleProfile#ConstraintsResource
:ConstraintsResource rdf:type owl:NamedIndividual ,
                              prof:ResourceDescriptor ;
                     dct:Format <https://www.w3.org/TR/turtle/> ;
                     prof:hasArtifact "./ExampleShapeWithPropertyShape.ttl" ;
                     prof:hasRole role:constraints .

:ConstraintsResource2 rdf:type owl:NamedIndividual ,
                              prof:ResourceDescriptor ;
                     dct:Format <https://www.w3.org/TR/turtle/> ;
                     prof:hasArtifact "./AnotherConstraintsFile.ttl" ;
                     prof:hasRole role:constraints .


###  https://w3id.org/schematransform/ExampleProfile#Profile
:Profile rdf:type owl:NamedIndividual ,
                  prof:Profile ;
         prof:hasResource :ConstraintsResource ,
                          :VocabularyResource .


###  https://w3id.org/schematransform/ExampleProfile#VocabularyResource
:VocabularyResource rdf:type owl:NamedIndividual ,
                             prof:ResourceDescriptor ;
                    dct:Format <https://www.w3.org/TR/turtle/> ;
                    prof:hasArtifact "./ExampleVocabulary.ttl" ;
                    prof:hasRole role:vocabulary .


